data1 = (1, 2, 3)
data2 = {4, 5, 6}
data3 = [7, 8, 9]


def function(x, y, z):
    convert1 = list(x)
    convert2 = tuple(y)
    convert3 = set(z)

    x = type(convert1)
    y = type(convert2)

    return x, y, z

    #print(convert1, convert2)
    #print(type(convert2))
    #print(convert3)


